{
  "rewrites": [
    { "source": "/", "destination": "/login" }
  ],
  "redirects": [
    { "source": "/home", "destination": "/login", "statusCode": 301 }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "max-age=0, must-revalidate" }
      ]
    }
  ]
}
